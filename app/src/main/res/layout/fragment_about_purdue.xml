<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/AboutLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/aboutLabel"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="67dp" />

    <Button
        android:id="@+id/mypurdue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mypurdue"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="102dp" />

    <Button
        android:id="@+id/bbbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Blackboard"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="102dp" />

    <Button
        android:id="@+id/dismiss"
        android:layout_width="56dp"
        android:layout_height="47dp"
        android:text="@string/X"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="53dp" />

    <TextView
        android:id="@+id/aboutparagraph"
        android:layout_width="378dp"
        android:layout_height="354dp"
        android:text=""
        android:textSize="14sp"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/purduehome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/purduehome"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="100dp" />
</androidx.constraintlayout.widget.ConstraintLayout>